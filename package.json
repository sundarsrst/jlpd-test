{
  "name": "ee-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "git@github.com:glean-notes-recruitment/ee-task.git",
  "author": "Alyssa <alyssa.burlton@glean.co>",
  "license": "MIT",
  "engines": {
    "node": "16.15.0",
    "npm": "npm is deprecated for this project - please use yarn"
  },
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.build.json && cp -r ./src/public ./dist/public",
    "preserve": "yarn run build",
    "serve": "concurrently \"tsc --project tsconfig.build.json --watch\" \"nodemon -q dist/index.js\"",
    "prettier-check": "prettier --check 'src/**/*.ts'",
    "prettier-format": "prettier 'src/**/*.ts' --write",
    "lint": "eslint . --ext .ts",
    "prestart": "yarn run build",
    "start": "node dist/index.js",
    "test": "jest"
  },
  "dependencies": {
    "@types/body-parser": "1.19.2",
    "@types/express": "4.17.13",
    "@types/node": "18.0.6",
    "@types/node-fetch": "2.6.2",
    "body-parser": "1.20.0",
    "express": "4.18.1",
    "helmet": "5.1.0",
    "node-fetch": "2.6.7",
    "rimraf": "3.0.2",
    "typescript": "4.7.4"
  },
  "devDependencies": {
    "@babel/preset-env": "7.18.9",
    "@babel/preset-typescript": "7.18.6",
    "@types/async-lock": "1.1.5",
    "@types/jest": "28.1.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "async-lock": "1.3.2",
    "concurrently": "7.2.2",
    "eslint": "8.20.0",
    "jest": "28.1.3",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "supertest": "6.2.4"
  }
}
